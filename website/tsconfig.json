{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "jsx": "preserve",
    "strict": true,
    "removeComments": false,
    "module": "nodenext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "alwaysStrict": false,
    "baseUrl": "..",
    "types": ["@testing-library/jest-dom"],
    "paths": {}
  }
}
